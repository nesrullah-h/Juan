<body>
    <header id="MainNav">
        <div class="container">
            <nav class="navbar navbar-expand-lg navbar-light">
                <a href="" class="nav-icon mx-2"><i class="fas fa-mobile-alt"></i></a> <a class="nav-text"> App<span>Bay</span></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto mr-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="#Cards">Features <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#SayPeople">Testtimonilas</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#ContactUs">Contact Us</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled">Disabled</a>
                        </li>
                        @if (User.Identity.IsAuthenticated)
                        {
                            <li class="nav-item">
                                <a class="nav-link disabled">@User.Identity.Name</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link disabled" asp-action="Account" asp-controller="LogOut">LogOut</a>
                            </li>
                        }
                    </ul>
                </div>
            </nav>
        </div>
    </header>
